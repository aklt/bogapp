{
  "name": "bogapp",
  "license": "CC-BY-NC-SA-4.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "pretty": "prettier --write --print-width 72 --single-quote --no-bracket-spacing --jsx-bracket-same-line pages/* components/* lib/*",
    "export": "next build && next export",
    "build-doc":"documentation build components/*.js pages/*.js lib/*.js -f html -g true -o out/codedoc",
    "gh-pages": "gh-pages -d out",
    "release": "npm run export && npm run documentation && npm run gh-pages && rm -rf out"
  },
  "dependencies": {
    "jest": "^20.0.1",
    "next": "^3.0.0-beta3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "documentation": "4.0.0-rc.1",
    "gh-pages": "^1.0.0",
    "prettier": "^1.2.2"
  }
}
